{
  "name": "tv-shows-lambda",
  "version": "0.1.0",
  "license": "MIT",
  "bin": {
    "tv-shows-lambda": "bin/tv-shows-lambda.js"
  },
  "scripts": {
    "build": "yarn lambda:build",
    "watch": "tsc -w",
    "test": "yarn lambda:build; jest --coverage",
    "cdk": "cdk",
    "tsc": "tsc -p tsconfig.json --noEmit",
    "datadog:synthetics": "datadog-ci synthetics run-tests --config ./.synthetics/datadog-ci.json",
    "lambda:build": "esbuild lambda/handlers/email.ts lambda/handlers/get.ts --bundle --outdir=dist --platform=node --sourcemap --minify --target=node12",
    "lambda:deploy": "cdk deploy --require-approval never",
    "prettier:check": "prettier --check \"**/*.*\"",
    "prettier:fix": "prettier --write \"**/*.*\"",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.95.1",
    "@aws-cdk/aws-apigateway": "1.95.1",
    "@aws-cdk/aws-certificatemanager": "1.95.1",
    "@aws-cdk/aws-dynamodb": "1.95.1",
    "@aws-cdk/aws-events-targets": "1.95.1",
    "@aws-cdk/aws-lambda": "1.95.1",
    "@aws-cdk/aws-logs": "1.95.1",
    "@aws-cdk/aws-logs-destinations": "1.95.1",
    "@aws-cdk/aws-route53": "1.95.1",
    "@aws-cdk/aws-route53-targets": "1.95.1",
    "@aws-cdk/core": "1.95.1",
    "@datadog/datadog-ci": "^0.11.4",
    "@types/aws-lambda": "^8.10.73",
    "@types/jest": "^26.0.22",
    "@types/node": "14.14.37",
    "@types/node-fetch": "^2.5.8",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.19.0",
    "aws-cdk": "1.96.0",
    "datadog-cdk-constructs": "^0.1.8",
    "esbuild": "^0.11.5",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "~4.2.3"
  },
  "dependencies": {
    "aws-sdk": "^2.879.0",
    "date-fns": "^2.19.0",
    "node-fetch": "^2.6.1"
  }
}
