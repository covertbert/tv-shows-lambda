{
  "name": "tv-shows-lambda",
  "version": "0.1.0",
  "license": "MIT",
  "bin": {
    "tv-shows-lambda": "bin/tv-shows-lambda.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "tsc": "tsc -p tsconfig.json --noEmit",
    "lambda:build": "esbuild lambda/handlers/get.ts --bundle --outfile=dist/get.js --platform=node",
    "lambda:deploy": "cdk deploy",
    "prettier:check": "prettier --check \"**/*.*\"",
    "prettier:fix": "prettier --write \"**/*.*\"",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.93.0",
    "@aws-cdk/aws-lambda": "^1.93.0",
    "@types/jest": "^26.0.20",
    "@types/node": "14.14.34",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.17.0",
    "aws-cdk": "1.93.0",
    "esbuild": "^0.9.2",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "typescript": "~4.2.3"
  },
  "dependencies": {
    "@aws-cdk/core": "1.93.0",
    "date-fns": "^2.19.0",
    "got": "^11.8.2",
    "source-map-support": "^0.5.19"
  }
}
